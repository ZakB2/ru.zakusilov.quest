<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>QuestServlet</servlet-name>
        <servlet-class>ru.zakusilov.quest.servlets.QuestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestResultServlet</servlet-name>
        <servlet-class>ru.zakusilov.quest.servlets.QuestResultServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RestartServlet</servlet-name>
        <servlet-class>ru.zakusilov.quest.servlets.RestartServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestServlet</servlet-name>
        <url-pattern>/quest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestResultServlet</servlet-name>
        <url-pattern>/questResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RestartServlet</servlet-name>
        <url-pattern>/restart</url-pattern>
    </servlet-mapping>
</web-app>